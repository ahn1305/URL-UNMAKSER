 {  
  "manifest_version": 2,  
  "name": "URL-Unmasker",  
  "description": "Extension for urlunmasker",  
  "version": "1.0",  
  "browser_action": {  
   "default_icon": "logo.png",  
   "default_popup": "popup.html"  
  },  
  "permissions": [  
   "activeTab",  
   "storage" 
  ] ,
  "background": {
    "scripts": ["background.js"],
    "persistent": false

  }
 }  